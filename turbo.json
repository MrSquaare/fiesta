{
  "baseBranch": "origin/main",
  "npmClient": "pnpm",
  "pipeline": {
    "dev:android": {
      "cache": false,
      "persistent": true
    },
    "dev:ios": {
      "cache": false,
      "persistent": true
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:android": {
      "dependsOn": [],
      "outputs": ["dist/**", "android/app/build/**"]
    },
    "build:ios": {
      "dependsOn": [],
      "outputs": ["dist/**", "ios/App/build/**"]
    },
    "start": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false,
      "persistent": true
    },
    "fclean": {
      "cache": false,
      "persistent": true
    }
  }
}